	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		Alumno juan = new Alumno("Juan", "PÃ©rez", "02166884l", "658438528");
		String dato = "";
		
		juan.llamar();
		juan.llamarCabreado();
		
		dato = juan.sacarDatos("nombre");
		System.out.println(dato);
		
		dato = juan.sacarDatos("apellidos");
