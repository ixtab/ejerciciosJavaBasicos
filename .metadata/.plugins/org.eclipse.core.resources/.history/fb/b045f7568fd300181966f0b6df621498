
public class EjercicioFuncionPrimos {

	static boolean esPrimo(int a){
		boolean primo = true;
		int i = 2;
		while (i < a){
			if (a%i == 0){
				primo = false;
			}
			i++;
		}
		return primo;
	}
	
	public static void main(String[] args) {
		int a = 1;
		for (a = 1; a<=100; a++){
			if (esPrimo(a)){
				System.out.println(a);
			}
		}
	}

}
