package ejercicioFibonacci;

public class EjercicioFibonacci {

	static int Fibonacci(int x){
		if (x<3){
			return 1;
		}else {
			return Fibonacci(x-1) + Fibonacci(x-2) - Fibonacci(x-3);
		}
	}
	
	public static void main(String[] args) {
		
		int a = Fibonacci(8);
		System.out.println(a);
		
	}

}
