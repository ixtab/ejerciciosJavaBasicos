
public class EjercicioFuncionPrimos {

	static boolean esPrimo(int a){
		boolean primo = true;
		int i = 2;
		while (primo == false || i < a){
			if (a%i ==0){
				primo = false;
			} else {
				i++;
			}
		}
		return primo;
	}
	
	public static void main(String[] args) {
		int a = 1973;
		if (esPrimo(a)){
			System.out.println("Primo");
		} else {
			System.out.println("nooor");
		}
	}

}
