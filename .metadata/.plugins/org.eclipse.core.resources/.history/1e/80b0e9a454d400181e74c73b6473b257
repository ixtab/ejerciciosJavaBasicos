package ejercicioFibonacci;

public class EjercicioFibonacci {

	static int Fibonacci(int x){
		int a = 0;
		if (x<2){
			System.out.println("en menor de 2 a: " + a + " - x: " + x);
			return 1;
		}else {
			System.out.println("en mayor de 2 a: " + a + " - x: " + x);
			a = Fibonacci(x-1) + Fibonacci(x-2);
		}
		System.out.println("Fuera del condicional a: " + a + " - x: " + x);
		return a;
	}
	
	public static void main(String[] args) {
		
		int a = Fibonacci(6);
		System.out.println(a);
		
	}

}
